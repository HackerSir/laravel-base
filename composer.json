{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "arcanedev/log-viewer": "^4.4",
        "barryvdh/laravel-ide-helper": "^2.4",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "^3.3",
        "graham-campbell/throttle": "^6.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.5",
        "lavary/laravel-menu": "^1.7",
        "marvinlabs/laravel-html-bootstrap-4": "^0.7.0",
        "marvinlabs/laravel-html-font-awesome": "^1.0",
        "predis/predis": "^1.1",
        "recca0120/laravel-tracy": "^1.8",
        "santigarcor/laratrust": "^5.0",
        "spatie/laravel-backup": "^5.1",
        "thomaswelton/laravel-gravatar": "^1.1",
        "yajra/laravel-datatables-buttons": "^3.1",
        "yajra/laravel-datatables-html": "^3.5",
        "yajra/laravel-datatables-oracle": "^8.3",
        "yish/generators": "^2.0"
    },
    "require-dev": {
        "filp/whoops": "^2.1",
        "fzaninotto/faker": "^1.7",
        "laravel/browser-kit-testing": "^2.0",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^6.5",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "spatie/laravel-html"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
